///IF OR ELSE///
var age=30;
if (age <=10){
  console.log("YOU CAN EAT FROM THE KIDS MENU");
}else{
  console.log("YOU CAN EAT FROM THE Adults MENU");
}//
///IF ELSE IF FUNCTION////
var age=50;
if (age <=10){
  console.log("YOU CAN EAT FROM THE KIDS MENU");
   console.log("YOU are not old enougth to drive");
  console.log("YOU  you are not old  enoughth to rent a car");
}else if (age >=16 && age < 25){
  console.log("YOU CAN not EAT FROM THE KIDS MENU");
   console.log("YOU are  old enougth to drive");
  console.log("YOU  you are not  old  enoughth to rent a car");
}else if (age >=  25){
  console.log("YOU CAN not EAT FROM THE KIDS MENU");
   console.log("YOU are  old enougth to drive");
  console.log("YOU  you are  old  enoughth to rent a car");
}///
