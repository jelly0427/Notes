///SWITCH STATMENT///
var datapoint =6;
switch (typeof datapoint){
  case "string":
    console.log('its a string');
break;
  case "number":
console.log('it is a number!');
break;
  case "boolean":
console.log('it either true or false!');
break;
  default:
    console.log('no matches');
}
